<Window x:Class="ZoDream.RegexTool.Pages.ResultView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZoDream.RegexTool.Pages"
        mc:Ignorable="d"
        Title="是否复制文字" Height="200" Width="300" Loaded="Window_Loaded" 
        Icon="../Assets/find.ico" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="ContentTb" Grid.ColumnSpan="2" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True"/>
        <TextBlock x:Name="Savetb" Grid.Row="1" Grid.Column="0" />
        <Button x:Name="Yesbtn" Content="复制" Grid.Row="2" Grid.Column="0" Click="Yesbtn_Click" Height="40"/>
        <Button x:Name="Nobtn" Content="取消" Grid.Row="2" Grid.Column="1" Click="Nobtn_Click"/>
    </Grid>
</Window>
